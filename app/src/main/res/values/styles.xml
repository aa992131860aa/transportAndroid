<resources>
    <style name="custom_dialog2" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- Dialog的windowFrame框为无 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否漂现在activity上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 是否半透明 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 去除黑色边框的关键设置项 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 屏幕背景是否变暗 -->
        <item name="android:backgroundDimAmount">0.3</item>
    </style>


    <style name="pickerview_dialogAnim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/pickerview_dialog_scale_in</item>
        <item name="android:windowExitAnimation">@anim/pickerview_dialog_scale_out</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowFullscreen">true</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="time_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@android:color/white</item>
    </style>
    <!--ll    -->
    <style name="rootBasic">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingBottom">@dimen/offset_bottom_ll</item>
        <item name="android:paddingLeft">@dimen/offset_left_ll</item>
        <item name="android:paddingRight">@dimen/offset_right_ll</item>
        <item name="android:paddingTop">@dimen/offset_top_ll</item>
        <item name="android:background">@drawable/common_bg8</item>
    </style>

    <!--RelativeLayout-->
    <style name="rootBase">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingBottom">@dimen/offset_bottom</item>
        <item name="android:paddingLeft">@dimen/offset_left</item>
        <item name="android:paddingRight">@dimen/offset_right</item>
        <item name="android:paddingTop">@dimen/offset_top</item>
        <item name="android:background">@drawable/common_bg8</item>
    </style>

    <!--btn 蓝色 实心-->
    <style name="btnBlueSolid">
        <item name="android:layout_width">144dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:background">@drawable/selector_btn_blue</item>
        <item name="android:textColor">#346f6d</item>
        <item name="android:textSize">20sp</item>
    </style>
    <!--btn 蓝色 实心-->
    <style name="btnBlueSolid1">
        <item name="android:layout_width">144dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:background">@drawable/selector_btn_blue</item>
        <item name="android:textColor">#346f6d</item>
        <item name="android:textSize">20sp</item>
    </style>
    <!--btn 蓝色 空心-->
